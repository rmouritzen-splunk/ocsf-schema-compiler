
{
  "description": "The Splunk extended/enriched metadata object.",
  "extends": "metadata",
  "profiles": [
    "splunk/ba"
  ],
  "attributes": {
    "event_code": {
      "requirement": "optional"
    },
    "log_level": {
      "caption": "Log Level",
      "description": "The audit level at which an event was generated.",
      "type": "string_t"
    },
    "log_name": {
      "requirement": "recommended"
    },
    "log_version": {
      "requirement": "optional"
    },
    "log_provider": {
      "requirement": "recommended"
    },
    "loggers": {},
    "customer_uid": {
      "profile": "splunk/ba",
      "requirement": "recommended"
    },
    "source": {
      "profile": "splunk/ba",
      "requirement": "recommended"
    },
    "source_type": {
      "profile": "splunk/ba",
      "requirement": "recommended"
    },
    "tags": {
      "description": "The list of tags; <code>{key:value}</code> pairs associated to the event.",
      "requirement": "optional"
    }
  }
}
