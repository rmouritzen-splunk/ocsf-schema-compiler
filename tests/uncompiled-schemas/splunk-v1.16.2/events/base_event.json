{
  "caption": "Extended Base Event",
  "description": "The extended base event adds the inferred caller and target objects.",
  "extends": "base_event",
  "profiles": [
    "splunk/ba"
  ],
  "attributes": {
    "$include": [
      "profiles/ba.json"
    ],
    "duration": {
      "description": "The event duration or aggregate time, the amount of time the event covers from <code>start_time</code> to <code>end_time</code> in milliseconds.",
      "group": "occurrence",
      "requirement": "optional"
    },
    "message": {
      "group": "primary",
      "requirement": "recommended"
    },
    "metadata": {
      "group": "context",
      "requirement": "required"
    },
    "observables": {
      "group": "primary",
      "requirement": "recommended"
    },
    "raw_data": {
      "group": "context",
      "requirement": "optional"
    },
    "status": {
      "group": "primary",
      "requirement": "recommended"
    },
    "status_code": {
      "group": "primary",
      "requirement": "recommended"
    },
    "status_detail": {
      "group": "primary",
      "requirement": "recommended"
    },
    "status_id": {
      "group": "primary",
      "requirement": "recommended"
    },
    "time": {
      "group": "occurrence",
      "requirement": "required"
    },
    "type_name": {
      "group": "classification",
      "requirement": "optional"
    }
  }
}